datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Exercise {
  category        String
  id              Int    @id @default(autoincrement())
  max             Int
  name            String
  primaryMuscle   String
  secondaryMuscle String
}
